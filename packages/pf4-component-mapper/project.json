{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "@jakub007d/pf4-component-mapper",
  "root": "packages/pf4-component-mapper",
  "sourceRoot": "packages/pf4-component-mapper/src",
  "projectType": "library",
  "tags": [
    "type:component-mapper",
    "ui:patternfly",
    "scope:publishable",
    "platform:react"
  ],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": [
        "{projectRoot}/index.js",
        "{projectRoot}/esm",
        "{projectRoot}/*.d.ts"
      ],
      "options": {
        "command": "yarn build",
        "cwd": "packages/pf4-component-mapper"
      },
      "dependsOn": [
        "^build"
      ],
      "cache": true
    },
    "build:cjs": {
      "executor": "nx:run-commands",
      "outputs": [
        "{projectRoot}/*.js"
      ],
      "options": {
        "command": "yarn build:cjs",
        "cwd": "packages/pf4-component-mapper"
      },
      "cache": true
    },
    "build:esm": {
      "executor": "nx:run-commands",
      "outputs": [
        "{projectRoot}/esm"
      ],
      "options": {
        "command": "yarn build:esm",
        "cwd": "packages/pf4-component-mapper"
      },
      "cache": true
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "echo 'Tests skipped for @jakub007d release testing'"
      }
    },
    "typecheck": {
      "executor": "@nx/js:tsc",
      "options": {
        "main": "packages/pf4-component-mapper/src/index.js",
        "tsConfig": "packages/pf4-component-mapper/tsconfig.json",
        "outputPath": "packages/pf4-component-mapper/dist"
      },
      "outputs": [
        "{options.outputPath}"
      ],
      "cache": true
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn start",
        "cwd": "packages/pf4-component-mapper"
      }
    }
  },
  "implicitDependencies": [
    "@jakub007d/common"
  ]
}
