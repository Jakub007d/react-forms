{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "@jakub007d/parsers",
  "root": "packages/parsers",
  "sourceRoot": "packages/parsers/src",
  "projectType": "library",
  "tags": [
    "type:utility",
    "scope:private",
    "platform:react"
  ],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": [
        "{projectRoot}/dist"
      ],
      "options": {
        "command": "yarn build",
        "cwd": "packages/parsers"
      },
      "dependsOn": [
        "^build"
      ],
      "cache": true
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "echo 'Tests skipped for @jakub007d release testing'"
      }
    }
  },
  "implicitDependencies": [
    "@jakub007d/react-form-renderer"
  ]
}
